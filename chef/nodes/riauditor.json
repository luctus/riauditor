{
  "app": {
    "server_name": "_",
    "name": "riauditor",
    "dir": "/var/www/vhosts/riauditor/current",
    "env": "production"
  },
  "user": {
    "name": "ubuntu"
  },
  "nginx": {
    "version": "1.5.7",
    "log_dir": "/var/log/nginx",
    "user": "www-data",
    "init_style": "init",
    "install_method": "source",
    "client_max_body_size": "1m",
    "port": "80",
    "vhosts_dir": "/var/www/vhosts",
    "source": {
      "modules": [
        "nginx::http_stub_status_module",
        "nginx::http_gzip_static_module",
        "nginx::passenger"
      ]
    },
    "passenger": {
      "version": "4.0.29",
      "ruby": "/home/ubuntu/.rbenv/versions/2.1.0/bin/ruby",
      "root": "/home/ubuntu/.rbenv/versions/2.1.0/lib/ruby/gems/2.1.0/gems/passenger-4.0.29/"
    }
  },
  "rbenv": {
    "user_installs": [
      {
        "user": "ubuntu",
        "rubies": ["2.1.0"],
        "global": "2.1.0",
        "gems": {
          "2.1.0": [
            {"name": "bundler"},
            {"name": "passenger", "version": "4.0.29"}
          ]
        }
      }
    ]
  },
  "run_list": [
    "recipe[riauditor::production]",
    "recipe[riauditor::app]"
  ]
}
